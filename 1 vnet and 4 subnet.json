{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vnetName": {
      "type": "string",
      "defaultValue": "VNet1",
      "metadata": {
        "description": "Name of the Virtual Network"
      }
    },
    "vnetAddressPrefix": {
      "type": "string",
      "defaultValue": "10.0.0.0/16",
      "metadata": {
        "description": "VNET Address prefix"
      }
    },
    "subnet1Prefix": {
      "type": "string",
      "defaultValue": "10.0.0.0/28",
      "metadata": {
        "description": "Subnet 1 Prefix"
      }
    },
      "subnet1Name": {
        "type": "string",
        "defaultValue": "GatewaySubnet",
        "metadata": {
          "description": "Subnet 1 Name"
        }
      },
    "subnet2Prefix": {
      "type": "string",
      "defaultValue": "10.0.1.0/28",
      "metadata": {
        "description": "Subnet 2 Prefix"
      }
    },
    "subnet2Name": {
      "type": "string",
      "defaultValue": "SecurityTrust",
      "metadata": {
        "description": "Subnet 2 Name"
      }
    },
    "subnet3Prefix": {
      "type": "string",
      "defaultValue": "10.0.2.0/28",
      "metadata": {
        "description": "Subnet 3 Prefix"
      }
    },
    "subnet3Name": {
      "type": "string",
      "defaultValue": "SecurityUntrust",
      "metadata": {
        "description": "Subnet 3 Name"
      }
    },
    "subnet4Prefix": {
      "type": "string",
      "defaultValue": "10.0.3.0/28",
      "metadata": {
        "description": "Subnet 4 Prefix"
      }
    },
    "subnet4Name": {
      "type": "string",
      "defaultValue": "SecurityMGMT",
      "metadata": {
        "description": "Subnet 4 Name"
      }
    },
    "subnet5Prefix": {
      "type": "string",
      "defaultValue": "10.0.4.0/24",
      "metadata": {
        "description": "Subnet 5 Prefix"
      }
    },
    "subnet5Name": {
      "type": "string",
      "defaultValue": "SecurityTrust",
      "metadata": {
        "description": "Subnet 5 Name"
      }
    }
  },
   "resources": [
    {
      "apiVersion": "2017-06-01",
      "type": "Microsoft.Network/virtualNetworks",
      "name": "[parameters('vnetName')]",
      "location": "[resourceGroup().location]",
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "[parameters('vnetAddressPrefix')]"
          ]
        },
        "subnets": [
          {
            "name": "[parameters('subnet1Name')]",
            "properties": {
              "addressPrefix": "[parameters('subnet1Prefix')]"
            }
          },
          
            {
              "name": "[parameters('subnet2Name')]",
              "properties": {
                "addressPrefix": "[parameters('subnet2Prefix')]"
              }
            },
            
              {
                "name": "[parameters('subnet3Name')]",
                "properties": {
                  "addressPrefix": "[parameters('subnet3Prefix')]"
                }
              },
              
                {
                  "name": "[parameters('subnet4Name')]",
                  "properties": {
                    "addressPrefix": "[parameters('subnet4Prefix')]"
                  }
                },
                {
                  "name": "[parameters('subnet5Name')]",
                  "properties": {
                    "addressPrefix": "[parameters('subnet5Prefix')]"
                  }
                }
              ]
      }
    }
  ]
}